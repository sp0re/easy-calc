{
    "presets": [
        "minify",
        [
            "@babel/env",
            {
                "targets": {
                    "browsers": [
                        "> 1%",
                        "ie >= 11"
                    ]
                },
                // "modules": false,
                "modules": "commonjs",
                "useBuiltIns": "usage",
                "corejs": {
                    "version": "3.8",
                    "proposals": true
                }
                // "forceAllTransforms": true, 
            }
        ],
        "@babel/preset-typescript"
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime",
            {
                // "helpers": false,
                "regenerator": true,
                "corejs": 3
            }
        ]
    ]
}