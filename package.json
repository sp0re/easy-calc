{
  "name": "easy-calc",
  "version": "0.0.1",
  "description": "一个使用简单的、解决JavaScript基本运算与四舍五入时精度丢失问题的方法",
  "main": "lib/index.js",
  "scripts": {
    "watch": "rimraf lib && tsc --watch",
    "build": "npm run build-dist && npm run build-lib",
    "build-lib": "rimraf lib && tsc && babel src --config-file babel.config.json --extensions .js,.jsx,.ts,.tsx --out-dir lib",
    "build-dist": "rimraf dist && webpack",
    "rise": "npm link"
  },
  "repository": {
    "type": "git",
    "url": "default"
  },
  "keywords": [
    "float",
    "arithmetic",
    "tofixed",
    "calc",
    "decimal",
    "bignumber",
    "add",
    "plus",
    "sub",
    "minus",
    "mul",
    "divide"
  ],
  "author": "sp0re",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "babel-loader": "^8.2.2",
    "babel-preset-minify": "^0.5.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.1.0",
    "typescript": "^4.3.4",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.6",
    "@babel/runtime-corejs3": "^7.14.7",
    "@types/big.js": "^6.1.1",
    "big.js": "^6.1.1",
    "core-js": "^3.15.2"
  }
}
